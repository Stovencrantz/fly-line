  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.0",
    "nodemon": "^2.0.6"
